{
  "name": "timeline-portfolio",
  "version": "1.0.0",
  "private": true,
  "description": "My portfolio to display the projects I have made in a timeline format.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Squibs/timeline-portfolio"
  },
  "license": "ISC",
  "author": "Zachary Holman",
  "scripts": {
    "build": "npm run clean && gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "serve": "gatsby serve -H 0.0.0.0",
    "start": "npm run clean && npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "postinstall": "patch-package"
  },
  "prettier": "@squibs/prettier-config",
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "@squibs/eslint-config-typescript-react"
    ],
    "ignorePatterns": [
      "gatsby-*.js"
    ]
  },
  "dependencies": {
    "@types/lodash": "^4.14.168",
    "@types/react-redux": "^7.1.16",
    "@types/styled-components": "^5.1.7",
    "babel-plugin-styled-components": "^1.12.0",
    "gatsby": "^2.32.4",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^2.12.0",
    "gatsby-plugin-offline": "^3.10.0",
    "gatsby-plugin-page-creator": "^2.10.0",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-sharp": "^2.14.1",
    "gatsby-plugin-styled-components": "^3.10.0",
    "gatsby-plugin-transition-link": "^1.20.5",
    "gatsby-plugin-use-query-params": "^1.0.1",
    "gatsby-source-filesystem": "^2.11.0",
    "gatsby-transformer-remark": "^2.16.0",
    "gatsby-transformer-sharp": "^2.12.0",
    "gsap": "^3.6.0",
    "iframe-resizer-react": "^1.1.0",
    "lodash": "^4.17.21",
    "patch-package": "^6.4.7",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-feather": "^2.0.9",
    "react-helmet": "^6.1.0",
    "react-loader-spinner": "^4.0.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.9",
    "styled-components": "^5.2.1",
    "styled-normalize": "^8.0.7",
    "typescript": "^4.1.5",
    "use-query-params": "^1.2.0"
  },
  "devDependencies": {
    "@squibs/editor-config": "^1.0.1",
    "@squibs/eslint-config-typescript-react": "^1.0.1",
    "@squibs/prettier-config": "^1.0.0",
    "@squibs/ts-config": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1"
  }
}
